mixin socials(data)
  .socials&attributes(attributes)
    ul.socials__list
      each item in data
        li.socials__item
          a.socials__link(href=item.href, rel="nofollow noopener noreferrer", aria-label="Переход на наши социальные сети")
            +icon(item.iconName, item.iconWidth, item.iconHeight).socials__icon
