mixin list-link(data)
  .list-link&attributes(attributes)
    +title({tag: "h3", text: data.title}).title--h3.title--600
    ul.list-link__list
      each item in data.filling
        li.list-link__item
          +link({href: item.href, text: item.text})(aria-label="Переход по ссылке для дополнительной информации").list-link__link
